Create a rally!

<%= form_for ([@movement, @rally]) do |form| %>
  <%= form.label :name %> 
  <%= form.text_field :name %>
 
  <%= form.label :address %> 
  <%= form.text_field :address %>
 
  <%= form.label :city %> 
  <%= form.text_field :city %>

  <%= form.label :zip %> 
  <%= form.text_field :zip %>

  <%= form.label :coordinator %>
  <%= form.select :coordinator, options_for_select([current_user.email]) %>

  <%= form.label :notes %>
	<%= form.text_field :notes %>

  <%= form.submit %>
<% end %>


