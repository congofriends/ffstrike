Your Movement!
<%= form_for @movement do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :story %>
  <%= form.text_area :story %>
  <%= form.label :category %>
  <%= form.text_field :category %>

  <%= form.label :image %>
  <%= image_tag @movement.image.url, size: "490x275" %>

  <%= form.label :video %>
  <iframe width="560" height="315" src="<%= @movement.video %>" frameborder="0" allowfullscreen></iframe>
<% end %>


