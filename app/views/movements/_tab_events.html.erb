<div class="tab-pane" id="events">
  <div class="well">
    <legend>Event Details</legend>
    <div class="panel panel-default">
      <div class="panel-body">
        <h4 class = "event_dash_legend">Select an Event to Edit</h3>
        <div class= "col-left">
       		<% @events.each do |e| %>
       		<button class= "edit-event-dash btn-block btn-info" id= "<%=e.id%>">
       			<span class= "event-img-dash"><%= image_tag(e.image(:medium), width: 50) %></span>&nbsp;&nbsp;
       			<span class="event-name-dash"><%= e.name %></span>
       		</button>
       		<% end %>
      	</div>
        <div class= "col-right">
        	<%= render partial: 'events/edit_event', collection: @events, as: :event %>
        </div>
      </div>
    </div>
  </div>
</div>
