$("#assign_volunteer<%= @event.id %>").bind('ajax:success', function(){
  // $(this).html("<%= @attendance.point_person ? image_tag(image_url('check_box.png'), class: 'check-box') : image_tag(image_url('uncheck_box.png'), class: 'check-box') %>");
  <% if @attendance.point_person %>
		$(this).text('Y');
 		$("#tasks_for_event_<%=@event.id %>").show();
  <% else %>
		$(this).text('N');
 		$("#tasks_for_event_<%=@event.id %>").hide();
  <% end %>
});
