
@import 'colors';

.events-button img, .network-button img {
  padding-bottom: 20px;
  width: 100%;
}

blockquote {
  margin-left: 20px;
  margin-top: 10px;
  color: $text-color;
}

.event-datetime {
  font-size: 13px;
  font-weight: 300;
  line-height: 1.6em;
}

.event-list {
  margin-bottom: 10px;
  border-style: solid;
  border-color: #ECECEC;
  border-width: thin;
  border-radius: 10px;
  padding-bottom: 10px;
  padding-top: 10px;

}

body {
  background-color: $background-color;
}

.spacer10 { height: 10px; width: 100%; font-size: 0; margin: 0; padding: 0; border: 0; display: block; }
