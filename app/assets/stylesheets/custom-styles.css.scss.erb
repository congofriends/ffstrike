  background: $background-color;
  border: 2px solid #fff;
  overflow: auto;
}
.modal-spinner {
  background: url(<%= asset_path "spinner.gif" %>) #111 no-repeat center center;
}

.modal a.close-modal {
  margin: 10px;
  background: url(<%= asset_path "close.png" %>) no-repeat 7px 7px;
}

