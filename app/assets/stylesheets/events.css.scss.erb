@import 'colors';

// event tab styles in movement dashboard

.col-left{
  width: 40%;
  float: left;
  background-color: #DCF3E7;
  max-height: 600px;
  overflow:scroll;
  padding: 10px;
}

.edit-event-dash{
  float: left;
}

.event-img-dash{
  float: left;
}

.edit-event {
  display: none;
  background-color: #f5f5f5;
  padding: 10px;
  margin: 10px;
}

.col-sm-1 {
  min-width: 6em;
}

.col-sm-10 {
  width: 70%;
}

.col-right {
  background-color: #DCF3E7;
  width: 55%;
  float: right;
}
