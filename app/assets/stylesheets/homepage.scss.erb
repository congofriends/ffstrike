.front-page {
  height: 150px;
}

.homepage-banner {
  width: 100%;
  height: auto;
  padding-bottom: 150px;
  background-image: 
  -webkit-linear-gradient(bottom, 
    rgba(255,255,255,1) 0%, 
    rgba(255,255,255,0) 20%
  ),url(<%= asset_path 'cat.gif' %>);
  background-repeat: no-repeat;
  background-position: center 45%;
}

.homepage-jumbotron {
}

.homepage-jumbotron-text {
  h1 { 
    font-size: 40px; 
    color: #eb6864; 
  }
  p { font-size: 20px; }
  margin-top: 50px;
  margin-bottom: 0px;
}
